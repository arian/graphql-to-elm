{
  "name": "graphql-to-elm",
  "version": "0.6.1",
  "description": "Validates graphql queries and converts them to elm code",
  "files": [
    "elm",
    "lib",
    "src",
    "index.js",
    "index.d.ts"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "dependencies": {
    "mkdirp": "^0.5.1"
  },
  "peerDependencies": {
    "graphql": "~0.12.0 || ~0.13.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.0",
    "@types/graphql": "^0.13.4",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^10.9.0",
    "@types/phantom": "^3.2.5",
    "@types/rimraf": "^2.0.2",
    "@types/tape": "^4.2.32",
    "body-parser": "^1.18.3",
    "elm": "^0.18.0",
    "express": "^4.16.3",
    "glob": "^7.1.2",
    "graphql": "^0.13.2",
    "apollo-server-express": "^2.0.4",
    "phantom": "^6.0.3",
    "rimraf": "^2.6.2",
    "tap-diff": "^0.1.1",
    "tape": "^4.9.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1"
  },
  "scripts": {
    "build": "rimraf lib && tsc",
    "docs": "elm-make --docs=documentation.json",
    "examples:build": "cd examples/basic && npm run build && cd ../basic2 && npm run build && cd ../features && npm run build",
    "examples:install": "cd examples/basic && npm install && cd ../basic2 && npm install && cd ../features && npm install",
    "prepublishOnly": "npm test && npm run build && npm run examples:build",
    "test:options": "tape -r ts-node/register test/optionsTest.ts | tap-diff",
    "test": "tape -r ts-node/register test/index.ts | tap-diff",
    "test:browser": "tape -r ts-node/register test/browserTest.ts | tap-diff",
    "test:gen": "tape -r ts-node/register test/genTest.ts | tap-diff",
    "test:gen-update": "tape -r ts-node/register test/genTest.ts --update | tap-diff"
  },
  "author": "Harm Boschloo",
  "repository": {
    "type": "git",
    "url": "git://github.com/harmboschloo/graphql-to-elm.git"
  },
  "homepage": "https://github.com/harmboschloo/graphql-to-elm",
  "keywords": [
    "graphql",
    "elm"
  ],
  "license": "BSD-3-Clause"
}
