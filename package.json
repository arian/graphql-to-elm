{
  "name": "graphql-to-elm",
  "version": "0.0.0",
  "description": "Validates graphql queries and converts them to elm code",
  "files": ["index.js"],
  "main": "index.js",
  "dependencies": {
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "@types/node": "^9.3.0",
    "body-parser": "^1.18.2",
    "chokidar": "^2.0.0",
    "elm": "^0.18.0",
    "express": "^4.16.2",
    "glob": "^7.1.2",
    "graphql": "^0.12.3",
    "graphql-server-express": "^1.3.2",
    "graphql-tools": "^2.20.0",
    "phantom": "^4.0.12",
    "rimraf": "^2.6.2",
    "tap-diff": "^0.1.1",
    "tape": "^4.8.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2"
  },
  "peerDependencies": {
    "graphql": "^0.12.3"
  },
  "scripts": {
    "test": "npm run test:gen && npm run test:browser",
    "test:gen": "tape -r ts-node/register test/genTest.ts | tap-diff",
    "test:gen-update":
      "tape -r ts-node/register test/genTest.ts --update | tap-diff",
    "test:gen-watch": "ts-node test/genWatch.ts",
    "test:browser": "tape -r ts-node/register test/browserTest.ts | tap-diff",
    "start": "npm run test:gen-watch"
  },
  "author": "Harm Boschloo",
  "repository": {
    "type": "git",
    "url": "git://github.com/harmboschloo/graphql2elm.git"
  },
  "homepage": "https://github.com/harmboschloo/graphql2elm",
  "keywords": ["graphql", "elm"],
  "license": "BSD3"
}
