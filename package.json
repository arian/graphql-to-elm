{
  "name": "graphql-to-elm",
  "version": "0.3.0",
  "description": "Validates graphql queries and converts them to elm code",
  "files": [
    "elm",
    "lib",
    "src"
  ],
  "main": "lib/index.js",
  "dependencies": {
    "mkdirp": "^0.5.1"
  },
  "peerDependencies": {
    "graphql": "^0.12.3"
  },
  "devDependencies": {
    "@types/graphql": "^0.12.4",
    "@types/node": "^9.4.6",
    "@types/rimraf": "^2.0.2",
    "@types/tape": "^4.2.32",
    "body-parser": "^1.18.2",
    "elm": "^0.18.0",
    "express": "^4.16.2",
    "glob": "^7.1.2",
    "graphql": "^0.12.3",
    "graphql-server-express": "^1.3.2",
    "graphql-tools": "^2.21.0",
    "phantom": "^4.0.12",
    "rimraf": "^2.6.2",
    "tap-diff": "^0.1.1",
    "tape": "^4.9.0",
    "ts-node": "^5.0.0",
    "typescript": "^2.7.2"
  },
  "scripts": {
    "test:gen": "tape -r ts-node/register test/genTest.ts | tap-diff",
    "test:gen-update": "tape -r ts-node/register test/genTest.ts --update | tap-diff",
    "test:gen-watch": "ts-node test/genWatch.ts",
    "test:browser": "tape -r ts-node/register test/browserTest.ts | tap-diff",
    "start": "npm run test:gen-watch",
    "test": "tape -r ts-node/register test/index.ts | tap-diff",
    "build": "rimraf lib && tsc",
    "build:examples": "npm run build --prefix examples",
    "docs": "elm-make --docs=documentation.json",
    "prepublishOnly": "npm test && npm run build && npm run build:examples"
  },
  "author": "Harm Boschloo",
  "repository": {
    "type": "git",
    "url": "git://github.com/harmboschloo/graphql-to-elm.git"
  },
  "homepage": "https://github.com/harmboschloo/graphql-to-elm",
  "keywords": [
    "graphql",
    "elm"
  ],
  "license": "BSD-3-Clause"
}
