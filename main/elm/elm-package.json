{
  "version": "1.0.0",
  "summary": "Validates graphql queries and converts them to elm code.",
  "repository": "https://github.com/harmboschloo/graphql-to-elm.git",
  "license": "BSD-3-Clause",
  "source-directories": ["src"],
  "exposed-modules": [
    "GraphQL.Helpers.Decode",
    "GraphQL.Helpers.Url",
    "GraphQL.Http.Basic",
    "GraphQL.Batch",
    "GraphQL.Errors",
    "GraphQL.Operation",
    "GraphQL.Optional",
    "GraphQL.PlainBatch",
    "GraphQL.Response"
  ],
  "dependencies": {
    "elm-lang/core": "5.1.1 <= v < 6.0.0",
    "elm-lang/http": "1.0.0 <= v < 2.0.0"
  },
  "elm-version": "0.18.0 <= v < 0.19.0"
}
