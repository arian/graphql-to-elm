{
  "name": "graphql-to-elm",
  "version": "0.6.1",
  "description": "Validates graphql queries and converts them to elm code",
  "files": [
    "elm",
    "lib",
    "src",
    "index.js",
    "index.d.ts"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "peerDependencies": {
    "graphql": "0.12.x || 0.13.x"
  },
  "dependencies": {
    "mkdirp": "^0.5.1"
  },
  "devDependencies": {
    "@types/graphql": "^0.13.4",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^10.9.0",
    "@types/rimraf": "^2.0.2",
    "elm": "0.19.0-bugfix2",
    "elm-test": "0.19.0-beta5",
    "rimraf": "^2.6.2",
    "tap-diff": "^0.1.1",
    "tape": "^4.9.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1"
  },
  "scripts": {
    "build": "rimraf lib && tsc",
    "docs": "elm-make --docs=documentation.json",
    "test": "tape -r ts-node/register test.ts | tap-diff",
    "test:elm": "tape -r ts-node/register test-elm.ts | tap-diff",
    "test:gen": "tape -r ts-node/register test-gen.ts | tap-diff",
    "test:gen-update": "tape -r ts-node/register test-gen-update.ts | tap-diff",
    "examples:build": "cd ../examples/basic && npm run build && cd ../basic2 && npm run build && cd ../features && npm run build",
    "prepublishOnly": "npm run build && npm run examples:build && npm test"
  },
  "author": "Harm Boschloo",
  "repository": {
    "type": "git",
    "url": "git://github.com/harmboschloo/graphql-to-elm.git"
  },
  "homepage": "https://github.com/harmboschloo/graphql-to-elm",
  "keywords": [
    "graphql",
    "elm"
  ],
  "license": "BSD-3-Clause"
}
